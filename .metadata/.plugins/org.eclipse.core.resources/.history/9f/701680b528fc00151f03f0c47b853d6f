package hu.elte;

import java.util.function.BiFunction;
import java.util.function.Consumer;
import java.util.function.Function;

public class Main {

	public static void main(String[] args){


			
			IntegerWrapper iw = new IntegerWrapper();
			iw.setValue(10);
			
			IntegerWrapper iw2 = new IntegerWrapper();
			iw2.setValue(100);
			
			iw.join(iw2, (a,b) -> a + b);
			
			System.out.println(iw.getValue());
			System.out.println(iw2.getValue());
			
			iw.map(a -> a + 1);
			System.out.println(iw.getValue());
			System.out.println(iw2.getValue());

	}
	
	
}
